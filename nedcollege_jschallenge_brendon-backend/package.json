{
  "name": "nedcollege_jschallenge_brendon-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "knex": "node ./node_modules/.bin/knex",
    "knex-sync-migrations": "node ./node_modules/.bin/knex -- migrate:latest",
    "knex-rollback-migrations": "node ./node_modules/.bin/knex -- migrate:rollback"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "mysql2": "^3.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.1.7",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}
